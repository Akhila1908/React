import React,{useState} from 'react'

 const Dest = () => {
    
        const [city,setCity] = useState("bhimavarm")
        
        return(
            <h2>Destination: {city}</h2> 
          <h2> {city ? "bhimavaram":"hyderabad"}</h2>
        ) 
 }   
 export default Dest


App.js

